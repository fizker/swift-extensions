    "role" : "article",
    "roleHeading" : "Article",
    "title" : "Handle fractional seconds when encoding\/decoding Date in JSON"
  },
  "primaryContentSections" : [
    {
      "content" : [
        {
          "anchor" : "Overview",
          "level" : 2,
          "text" : "Overview",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "The built-in encoders and decoders do not support fractional seconds. This means that trying to parse dates created by most other languages will fail.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "The relevant standards are document here:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "items" : [
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "identifier" : "https:\/\/www.iso.org\/iso-8601-date-and-time-format.html",
                      "isActive" : true,
                      "type" : "reference"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "identifier" : "https:\/\/www.ietf.org\/rfc\/rfc3339",
                      "isActive" : true,
                      "type" : "reference"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            }
          ],
          "type" : "unorderedList"
        },
        {
          "anchor" : "Example-for-using-encoder-and-decoder",
          "level" : 2,
          "text" : "Example for using encoder and decoder",
          "type" : "heading"
        },
        {
          "code" : [
            "func parseData(_ data: Data) throws -> MyType {",
            "\tlet decoder = JSONDecoder()",
            "\tdecoder.dateDecodingStrategy = .iso8601WithFractionalSeconds",
            "\treturn try decoder.decode(MyType.self, from: data)",
            "}",
            "",
            "func encodeData(_ myType: MyType) throws -> Data {",
            "\tlet encoder = JSONEncoder()",
            "\tencoder.dateEncodingStrategy = .iso8601WithFractionalSeconds",
            "\treturn try encoder.encode(myType)",
            "}"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        },
        {
          "anchor" : "Example-for-converting-manually",
          "level" : 2,
          "text" : "Example for converting manually",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Create a ",
              "type" : "text"
            },
            {
              "code" : "Date",
              "type" : "codeVoice"
            },
            {
              "text" : " from a string. The init returns ",
              "type" : "text"
            },
            {
              "code" : "nil",
              "type" : "codeVoice"
            },
            {
              "text" : " if the date could not be parsed:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "if let date = Date(iso8601: \"2019-03-22T13:58:24.371Z\") {",
            "\tassert(date.timeIntervalSince1970, 1_553_263_104.371)",
            "}"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "Converting a ",
              "type" : "text"
            },
            {
              "code" : "Date",
              "type" : "codeVoice"
            },
            {
              "text" : " to a string",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "let date = Date(timeIntervalSince1970: 1_553_263_104.371)",
            "assert(.iso8601, \"2019-03-22T13:58:24.371Z\")"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        }
      ],
      "kind" : "content"
    }
