        {
          "inlineContent" : [
            {
              "text" : "This is a convenience operator. It allows to pass a value through functions without wrapping the value in parantheses.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "\/\/ given",
            "func increase(value: Int) -> Int { value + 1 }",
            "",
            "\/\/ Instead of",
            "increase(increase(1)) == 3",
            "",
            "\/\/ we can now do",
            "1 |> increase |> increase == 3"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        }
      ],
      "kind" : "content"
    }
